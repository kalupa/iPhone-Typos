<h1>New Typo</h1>

<% form_for(@typo) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :html, "Typo" %><br />
    <%= f.text_area :html %>
  </p>
  <p>
    <%= f.label :name, "Your Name" %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', typos_path %>